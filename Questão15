#include <iostream>

using namespace std;

int main() {
    float nota;
    float* ptrNota = &nota;

    cout << "Digite a nota do aluno: ";
    cin >> *ptrNota;

    if (*ptrNota >= 7.0)
        cout << "Aluno aprovado!" << endl;
    else
        cout << "Aluno reprovado." << endl;

    return 0;
}
