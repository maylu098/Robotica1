#include <iostream>

using namespace std;

float calcularAreaRetangulo(float base, float altura) {
    return base * altura;
}

int main() {
    float base, altura;
    cout << "Digite a base e a altura do retangulo: ";
    cin >> base >> altura;

    float area = calcularAreaRetangulo(base, altura);
    cout << "Area do retangulo: " << area << endl;

    return 0;
}
